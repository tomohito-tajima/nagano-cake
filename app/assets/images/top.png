      function resize_iframe(mode){
        $("#news iframe, #news script").each(function(){
          var width = $(this).width();
          var height = $(this).height();
          if(mode == "first"){
            $(this).css("max-width", width)
            $(this).attr("tmp-width", width)
            $(this).attr("tmp-height", height)
            $(this).attr("tmp-ratio", height/width)
          }
          if($("#news").width() < Number($(this).attr("tmp-width"))){
            $(this).attr({"width": "100%"})
            var full_width = $(this).width()
            height = Math.floor(full_width*Number($(this).attr("tmp-ratio")))
            $(this).attr({"height": height});
          }
        });
      }
      resize_iframe("first")
      $(window).resize(function(){
        resize_iframe("")
      })
    }
